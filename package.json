{
  "name": "koa-books-store-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -L --inspect=0.0.0.0 app/server.js",
    "test": "jest --forceExit --detectOpenHandles",
    "generate:keys": "node ./bin/generate-keys.js",
    "db:seed": "node ./bin/db-seed.js",
    "db:clear": "node ./bin/db-clear.js",
    "storage:clear": "node ./bin/storage-clear.js",
    "lint": "./node_modules/.bin/eslint ./app/**/* ./test/**/* ./bin/**/*",
    "lint:fix": "./node_modules/.bin/eslint ./app/**/* ./test/**/* ./bin/**/* --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.5",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "faker": "^4.1.0",
    "inquirer": "^6.2.2",
    "jest": "^24.7.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "lodash": "^4.17.11",
    "mime": "^2.4.2",
    "module-alias": "^2.2.0",
    "mongoose": "^5.4.21",
    "mongoose-unique-validator": "^2.0.2",
    "node-rsa": "^1.0.5",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "validator": "^10.11.0"
  },
  "_moduleAliases": {
    "@root": ".",
    "@routes": "./app/http/routes",
    "@controllers": "./app/http/controllers",
    "@middlewares": "./app/http/middlewares",
    "@database": "./app/database",
    "@models": "./app/database/models",
    "@serializers": "./app/serializers",
    "@services": "./app/services",
    "@errors": "./app/errors",
    "@config": "./app/config",
    "@utils": "./app/utils",
    "@views": "./app/views"
  },
  "jest": {
    "verbose": true,
    "globalSetup": "<rootDir>/test/setup.js",
    "globalTeardown": "<rootDir>/test/teardown.js",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@root/(.*)": "<rootDir>/$1",
      "@routes/(.*)": "<rootDir>/app/http/routes/$1",
      "@middlewares/(.*)": "<rootDir>/app/http/middlewares/$1",
      "@controllers/(.*)": "<rootDir>/app/http/controllers/$1",
      "@database/(.*)": "<rootDir>/app/database/$1",
      "@models/(.*)": "<rootDir>/app/database/models/$1",
      "@serializers/(.*)": "<rootDir>/app/serializers/$1",
      "@services/(.*)": "<rootDir>/app/services/$1",
      "@errors/(.*)": "<rootDir>/app/errors/$1",
      "@config/(.*)": "<rootDir>/app/config/$1",
      "@utils/(.*)": "<rootDir>/app/utils/$1",
      "@views/(.*)": "<rootDir>/app/views/$1"
    }
  }
}
